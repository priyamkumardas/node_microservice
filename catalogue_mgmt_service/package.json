{
  "name": "master_catalogue_service",
  "version": "1.0.0",
  "description": "Master Catalogue Service",
  "main": "./server.js",
  "scripts": {
    "prd": "node ./server",
    "stg": "nodemon ./server",
    "lcl": "nodemon -r dotenv/config ./server dotenv_config_path=./.lcl.env",
    "lcl:prd": "nodemon -r dotenv/config ./server dotenv_config_path=./.prd.env",
    "lcl:stg": "nodemon -r dotenv/config ./server dotenv_config_path=./.stg.env",
    "lcl:dev": "nodemon -r dotenv/config ./server dotenv_config_path=./.dev.env",
    "test": "NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=test mocha ./unitTests/ --recursive",
    "mdb:latest-dev": "node -r dotenv/config ./src/scripts/migrateLatest.js dotenv_config_path=./.dev.env",
    "mdb:latest-dev-mata": "node -r dotenv/config ./src/scripts/migrateLatestMeta.js dotenv_config_path=./.dev.env",
    "mdb:make-lcl": "node -r dotenv/config ./src/scripts/MigrateMake.js dotenv_config_path=./.env.lcl",
    "mdb:rollback-dev": "node -r dotenv/config ./src/scripts/migrateRollback.js dotenv_config_path=./.dev.env",
    "mdb:make-dev": "node -r dotenv/config ./src/scripts/migrateMake.js dotenv_config_path=./.dev.env",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Rishabh Raj",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "ajv": "^8.11.2",
    "ajv-formats": "^2.1.1",
    "aws": "^0.0.3-2",
    "aws-sdk": "^2.1270.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "cuid": "^3.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-json-validator-middleware": "^3.0.1",
    "fast-csv": "^4.3.6",
    "fs": "^0.0.1-security",
    "is-image-url": "^1.1.8",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongoose": "^6.3.5",
    "mongoose-id-assigner": "^4.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "node-fetch": "^3.3.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "sarvm-utility": "git+https://sushant.gautam%40sarvm.ai-at-326457620362:ro1XVb4vCYxaiPOlKO95OGNkApsH8SQREmiWUVdRT+c=@git-codecommit.ap-south-1.amazonaws.com/v1/repos/node_utility#v5.0.3",
    "short-unique-id": "^4.4.4",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13",
    "xhr2": "^0.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  },
  "_moduleAliases": {
    "@root": ".",
    "@controllers": "src/apis/controllers",
    "@services": "src/apis/services",
    "@db": "src/apis/db",
    "@models": "src/apis/models",
    "@routes": "src/apis/routes",
    "@constants": "src/constants",
    "@config": "src/config",
    "@common": "src/common",
    "db_meta": "src/apis/db_meta"
  }
