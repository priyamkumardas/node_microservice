{
  "env": {
    "commonjs": true,
    "node": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "globals": {
    "Configuration": "writable"
  },
  "rules": {
    "import/no-unresolved": [2, { "caseSensitive": false }]
  }
}
