    "ajv-formats": "^2.1.1",
    "aws-sdk": "^2.1360.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "cuid": "^3.0.0",
    "dotenv": "^16.0.1",
    "elastic-apm-node": "^3.43.0",
    "express": "^4.18.1",
    "express-json-validator-middleware": "^3.0.1",
    "fast-csv": "^4.3.6",
    "fs": "^0.0.1-security",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "knex-postgis": "^0.14.3",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongoose": "^6.10.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "sarvm-utility": "git+https://sushant.gautam%40sarvm.ai-at-326457620362:ro1XVb4vCYxaiPOlKO95OGNkApsH8SQREmiWUVdRT+c=@git-codecommit.ap-south-1.amazonaws.com/v1/repos/node_utility#v5.0.3",
