  "main": "./server.js",
  "scripts": {
    "prd": "node ./server",
    "stg": "nodemon ./server",
    "lcl": "nodemon -r dotenv/config ./server dotenv_config_path=./.lcl.env",
    "lcl:prd": "nodemon -r dotenv/config ./server dotenv_config_path=./.prd.env",
    "lcl:stg": "nodemon -r dotenv/config ./server dotenv_config_path=./.stg.env",
    "lcl:dev": "nodemon -r dotenv/config ./server dotenv_config_path=./.dev.env",
    "test": "NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=test mocha ./unitTests/ --recursive",
    "mdb:latest": "node -r dotenv/config ./src/scripts/migrateLatest.js dotenv_config_path=./.lcl.env",
    "mdb:make": "node -r dotenv/config ./src/scripts/migrateMake.js dotenv_config_path=./.lcl.env",
    "mdb:rb": "node -r dotenv/config ./src/scripts/migrateRollback.js dotenv_config_path=./.lcl.env",
    "mdb:seed": "node -r dotenv/config ./src/scripts/seedData.js dotenv_config_path=./.lcl.env"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
